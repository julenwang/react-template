    "webpack-dev-server": "^4.7.1"
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^3.1.4",
    "@reduxjs/toolkit": "^1.7.1",
    "ahooks": "^3.1.2",
    "axios": "^0.24.0",
    "clsx": "^1.2.1",
    "dayjs": "^1.10.7",
    "highlight.js": "^11.5.1",
    "i18next": "^21.6.14",
    "immer": "^9.0.7",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.16.2",
    "react-query": "^3.34.6",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.2.1",
    "react-use": "^17.3.2",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.7",
    "redux": "^4.1.2",
    "rxjs": "^7.5.5",
    "semver": "^7.3.7",
    "styled-components": "^5.3.3"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "jest": {
    "transform": {
      "\\.tsx?$": "ts-jest",
      "\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(molstar)/)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "isolatedModules": true
      }
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "browserslist": [
    "supports css-grid",
    "not IE > 0"
  ]
}
